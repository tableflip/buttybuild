{
  "name": "buttybuild",
  "version": "0.0.0",
  "description": "p2p CI",
  "main": "dist/server/main.js",
  "scripts": {
    "start": "electron .",
    "build": "run-p build:*",
    "build:js": "babel src --out-dir dist",
    "build:assets": "rsync src/./ dist --exclude=*.js --recursive",
    "watch": "npm-run-all build --parallel watch:* start",
    "watch:js": "npm run build:js -- --watch",
    "watch:assets": "nodemon --watch src -e css,html --exec \"npm run build:assets\"",
    "test": "run-s build test:*",
    "test:lint": "run-s lint",
    "test:functional": "tape 'test/**/*.test.js'",
    "lint": "standard",
    "clean": "rm -rf lib/**"
  },
  "keywords": [
    "ci",
    "build",
    "p2p",
    "scuttlebutt",
    "ssb"
  ],
  "author": "Alan Shaw",
  "dependencies": {
    "prop-types": "^15.6.0",
    "react": "^16.0.0",
    "react-dom": "^16.0.0",
    "react-router-dom": "^4.2.2",
    "tachyons": "^4.8.1",
    "tachyons-flexbox": "^2.0.5"
  },
  "devDependencies": {
    "babel-cli": "^6.26.0",
    "babel-eslint": "^8.0.1",
    "babel-preset-env": "^1.6.0",
    "babel-preset-react": "^6.24.1",
    "babel-preset-stage-2": "^6.24.1",
    "electron": "~1.7.8",
    "nodemon": "^1.12.1",
    "npm-run-all": "^4.1.1",
    "standard": "^10.0.3",
    "tape": "^4.8.0"
  },
  "standard": {
    "ignore": [
      "lib"
    ],
    "parser": "babel-eslint"
  }
}
